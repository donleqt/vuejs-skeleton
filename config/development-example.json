{
  "devServer": {
    "port": 5001
  },
  "nodeServer": {
    "port": 5000,
    "ssrAll": true,
    "useCachePage": false,
    "cacheTime": 5000,
    "logLevel": "info"
  },
  "build": {
    "mode": "production",
    "skipSSR": false,
    "watchSSR": false,
    "analyse": false,
    "outputDir": "dist"
  },
  "vueApp": {
    "publicPath": "/",
    "baseUrl": "/",
    "api": {
      "browserEndPoint": "http://api.production.info/api/v1",
      "nodeEndPoint": "http://localhost:3001/api/v1"
    }
  }
}
